                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">IF (Select count(*) from [Tesca EDW].TescaEdw.fact_PurchaseTransaction)&gt;0
	BEGIN

 select TransactionID, TransactionNo, Transdate, orderDate, deliveryDate,ShipDate,VendorID, EmployeeID, ProductID, StoreID,
		 Quantity, LineAmount, TaxAmount,GetDate() as LoadDate
		 from PurchaseTransaction   Where Convert(date,Transdate)=DATEADD(d,-1,convert(date,getdate()))   
	END
ELSE
          BEGIN

 select TransactionID, TransactionNo, Transdate, orderDate, deliveryDate,ShipDate,VendorID, EmployeeID, ProductID, StoreID,
 Quantity, LineAmount, TaxAmount,GetDate() as LoadDate
	 from PurchaseTransaction   Where Convert(date,Transdate)&lt;=DATEADD(d,-1,convert(date,getdate()))  

	 END</property>
